<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.nearbylocation">

    <uses-permission android:name="android.permission.INTERNET" />


    <application
        android:name="com.nearbylocation.App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.nearbylocation.activity.MainActivity"
            android:label="@string/title_main_activity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.nearbylocation.activity.FourSquareActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/title_four_square_activity">
        </activity>
        <activity
            android:name="com.nearbylocation.activity.GooglePlacesActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/title_google_places_activity">
        </activity>
    </application>

</manifest>